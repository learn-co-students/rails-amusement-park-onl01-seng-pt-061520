<% if flash[:message] %>
  <div>
    <p><%= flash[:message] %></p>
  </div>
<% end %>

<h1> Name: <%= @user.name %></h1>
<h1> Tickets: <%= @user.tickets %></h1>
<% if !@user.happiness.nil? && !@user.nausea.nil? %>
<h1> Mood: <%= @user.mood %></h1>
<% end %>
<h1> Height: <%= @user.height %>inches</h1>

<% if @user.admin %>
    <h1>ADMIN</h1>
<% end %>
<%= link_to  "See attractions", attractions_path %>
<%= link_to  "Log Out", logout_path, method: "post" %>


