<% if flash[:message] %>
  <div>
    <p><%= flash[:message] %></p>
  </div>
<% end %>

<h1><%= @user.name %></h1>

<% if @user.admin %>
  <h2>ADMIN</h2>
<% else %>
  <h2>Mood: <%= @user.mood %>
<% end %>

<h2>Tickets: <%= @user.tickets %></h2>
<h2>Height: <%= @user.height %></h2>

<%= link_to  "Log Out", logout_path, method: "post" %>

<%= link_to  "See attractions", attractions_path %>