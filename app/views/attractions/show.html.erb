<<% if flash[:message] %>
<div>
  <p><%= flash[:message] %></p>
</div>
<% end %>

<h1> Name: <%= @attraction.name %></h1>
<h1> Tickets: <%= @attraction.tickets %></h1>
<h1> Nausea Rating: <%= @attraction.nausea_rating %></h1>
<h1> Happiness Rating: <%= @attraction.happiness_rating %></h1>
<h1> Height Required: <%= @attraction.min_height %></h1>

<%=link_to  "See attractions", attractions_path %>
<%= button_to  "Go on this ride", rides_path(@attraction) %>


    
<% if @current_user.admin %>
<%= link_to  "Edit Attraction", edit_attraction_path %>

<% end %>


